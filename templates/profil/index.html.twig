{% extends 'base.html.twig' %}

{% block title %}Profil
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			padding: 2px 6px;
		}
	</style>

	<div class="example-wrapper">
	
		{% if app.user %}
			<div class="mb-3">
				{{ app.user.userIdentifier }},
				<a href="{{ path('app_logout') }}">Déconnexion</a>
			</div>
		{% endif %}


		<ul>
			Test de renvoie vers profil aprés connexion!!!
		</ul>
	</div>
{% endblock %}
